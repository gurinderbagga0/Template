<div class="modal fade" id="modelTwoFactorAuthentication" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Two Factor Authentication</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="formForgotPassword" class="simple_form session">
                    <div class="form-group">
                        <input type="email" class="form-control" placeholder="Code" id="txtTwoFactorAuthentication_Code" message="Email is a required field." required />
                    </div>
                    <div class="form-group">
                        <p style="color:red;">You will receive the Two Factor Authentication code on your email address or phone number.</p>
                    </div>

                    </div>
                </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="btnTwoFactorAuthentication"> Verify Me</button>
                <button type="button" class="btn btn-info" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>





