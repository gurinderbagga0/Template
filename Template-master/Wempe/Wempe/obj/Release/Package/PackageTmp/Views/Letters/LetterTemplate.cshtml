@{
    ViewBag.Title = "Letter Template";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


  <div class="page-bar">
    <ul class="page-breadcrumb">
        <li>
            <i class="fa fa-home"></i>
            <a href="#">Home</a>
            <i class="fa fa-angle-right"></i>
        </li>
        <li>
            <a href="#">Custom Template</a>
        </li>
    </ul>
</div>
<!-- END PAGE HEADER-->
<div id="Appraiser-list">

    <!-- BEGIN PAGE CONTENT-->


    <div class="row">
        <div class="col-md-12">
            <div class="portlet">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-grid"></i>Create New
                    </div>
                    <div class="actions">
                    </div>
                </div>


                <div class="portlet-body">
                    <div class="table-container">
                        <table class="table table-striped table-bordered table-hover" id="datatable_Brands">
                            <thead>
                                <tr role="row" class="heading">
                                    <th width="40%">
                                        Select Tags
                                    </th>
                                    <th width="10%">
                                        Actions
                                    </th>
                                </tr>
                                <tr role="row" class="filter">
                                    <td>
                                        <select id="ddl" class="form-control"><option>Select Tags</option><option value="{Tag1}">{Tag1}</option><option value="{Tag2}">{Tag2}</option></select>
                                    </td>
                                    <td class="mw_130">
                                        <div class="margin-bottom-5">
                                          
                                            <input type="button" id="btn" value="Save" class="btn btn-sm yellow filter-submit margin-bottom" />
                                            @*<a href="../User/create"> <button class="btn btn-sm green"><i class="fa fa-check"></i>Add New</button></a>*@
                                        </div>
                                    </td>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
                <div class="portlet-body">
                  
                 


                        <div class="col-md-12">

                           
                            <div name="summernote" id="summernote_1">
                            </div>

                           
                        </div>


                    </div>
            </div>
          
        </div>
    </div>
    <!-- END PAGE CONTENT-->




</div>



<!-- BEGIN PAGE LEVEL STYLES onclick="Add();"-->
<link href="~/Content/themes/admin/plugins/bootstrap-modal/css/bootstrap-modal-bs3patch.css" rel="stylesheet" type="text/css" />
<link href="~/Content/themes/admin/plugins/bootstrap-modal/css/bootstrap-modal.css" rel="stylesheet" type="text/css" />


@* New files for editor *@

<link rel="stylesheet" type="text/css" href="../../assets/global/plugins/bootstrap-wysihtml5/bootstrap-wysihtml5.css" />
<link rel="stylesheet" type="text/css" href="../../assets/global/plugins/bootstrap-summernote/summernote.css">

@* end new files *@


<!-- END PAGE LEVEL STYLES -->
@section scripts{

    <script src="~/Content/themes/admin/plugins/bootstrap-modal/js/bootstrap-modalmanager.js" type="text/javascript"></script>
    <script src="~/Content/themes/admin/plugins/bootstrap-modal/js/bootstrap-modal.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <script src="~/Content/themes/admin/pages/scripts/ui-extended-modals.js"></script>
   
    @* New Files for editor *@
<script type="text/javascript" src="../../assets/global/plugins/bootstrap-wysihtml5/wysihtml5-0.3.0.js"></script>
<script type="text/javascript" src="../../assets/global/plugins/bootstrap-wysihtml5/bootstrap-wysihtml5.js"></script>
<script src="../../assets/global/plugins/bootstrap-summernote/summernote.min.js" type="text/javascript"></script>
 <script src="../../assets/admin/pages/scripts/components-editors.js"></script>
    @* end new files *@

    <script>
        jQuery(document).ready(function () {

            ComponentsEditors.init();

            Metronic.init(); // init metronic core components
            Layout.init(); // init current layout
            QuickSidebar.init(); // init quick sidebar
            Demo.init(); // init demo features
            UIExtendedModals.init();

            $('#btn').click(function () {
                alert($('.note-editable').html());
            });
            $('#ddl').change(function () {
                var temp = $('.note-editable').html();
                temp += $('#ddl').val();
                $('.note-editable').html(temp);
            });
            //UIToastr.init();
        });


    </script>
}
