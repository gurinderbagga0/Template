<!-- BEGIN PAGE CONTENT-->
<div class="form-horizontal">
    <div class="form-body">
        
        <div class="row">

            <div class="col-md-2">
                <div class="form-group">
                    <label class="control-label col-md-3">TITLE</label>
                    <div class="col-md-6">
                        @Html.DropDownList("drpTitels1", (IEnumerable<SelectListItem>)ViewBag.NameTitles,"", new { @class = "form-control searchValue mw_50" ,@fieldName="title"})
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">FIRST</label>
                    <div class="col-md-9">
                        <input type="text" id="txtFirstName" fieldName="firstName" class="form-control searchValue" maxlength="100" />
                    </div>
                </div>
            </div>

            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">MIDDLE </label>
                    <div class="col-md-9">
                        <input type="text" id="txtMiddle" fieldName="middleInitial" class="form-control searchValue" maxlength="100" />
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">LAST</label>
                    <div class="col-md-9">
                        <input type="text" id="txtLast" fieldName="lastName" class="form-control searchValue" maxlength="100" />
                    </div>
                </div>
            </div>
            <div class="col-md-1">
                <div class="form-group last-col">
                    <label class="control-label col-md-3">(CUSTOMER)</label>
                </div>
            </div>

            @* Spouse *@


            <div class="col-md-2">
                <div class="form-group">
                    <label class="control-label col-md-3">TITLE</label>
                    <div class="col-md-6">
                        @Html.DropDownList("drpTitelsSpouse", (IEnumerable<SelectListItem>)ViewBag.NameTitles,"", new { @class = "form-control searchValue mw_50", @fieldName="asstTitle"})
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">FIRST </label>
                    <div class="col-md-9">
                        <input type="text" id="txtFirstNameSpouse" fieldName="SpousefirstName" class="form-control searchValue" maxlength="100" />
                    </div>
                </div>
            </div>

            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">MIDDLE </label>
                    <div class="col-md-9">
                        <input type="text" id="txtMiddleSpouse" fieldName="SpousemiddleInitial" class="form-control searchValue" maxlength="100" />
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">LAST </label>
                    <div class="col-md-9">
                        <input type="text" id="txtLastSpouse" fieldName="SpouselastName" class="form-control searchValue" maxlength="100" />
                    </div>
                </div>
            </div>
            <div class="col-md-1">
                <div class="form-group last-col">
                    @*<label class="control-label col-md-3">Relation</label>*@
                    <div class="col-md-12">
                        <select id="ddlSpouseRelation" class="form-control searchValue" fieldName="SpouseRelation">
                            <option value="0">Select</option>
                            <option value="Wife">Wife</option>
                            <option value="Spouse">Spouse</option>
                            <option value="Husband">Husband</option>
                            <option value="Assistant">Assistant</option>
                            <option value="Other">Other</option>
                        </select>
                    </div>
                </div>
            </div>

            @*row 2*@
            <div class="col-md-2">
                <div class="form-group">
                    <label class="control-label col-md-3">TITLE</label>
                    <div class="col-md-6">
                        @Html.DropDownList("drpTitels2", (IEnumerable<SelectListItem>)ViewBag.NameTitles,"", new { @class = "form-control searchValue mw_50", @fieldName="SpouseTitle" })
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">FIRST </label>
                    <div class="col-md-9">
                        <input type="text" id="txtFirstName1" fieldName="asstFirstName" class="form-control searchValue" maxlength="100" />
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">MIDDLE </label>
                    <div class="col-md-9">
                        <input type="text" id="txtMiddle1" fieldName="asstMiddleInitial" class="form-control searchValue" maxlength="100" />
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label col-md-3">LAST </label>
                    <div class="col-md-9">
                        <input type="text" id="txtLast1" fieldName="asstLastName" class="form-control searchValue" />
                    </div>
                </div>
            </div>
            <div class="col-md-1">
                <div class="form-group last-col">
                    @*<label class="control-label col-md-3">(ASSISTANT)</label>*@
                    <div class="col-md-12">
                        <select id="ddlAssistantRelation" class="form-control searchValue" fieldName="AsstRelation">
                            <option value="0">Select</option>
                            <option value="Wife">Wife</option>
                            <option value="Spouse">Spouse</option>
                            <option value="Husband">Husband</option>
                            <option value="Assistant">Assistant</option>
                            <option value="Other">Other</option>
                        </select>
                        </div>
                    </div>
            </div>

        </div>
        @*PRIMARY ADDRESS*@
        <div class="row">
            <div class="col-md-12 col-sm-10">
                <div class="portlet blue-hoki box">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="fa fa-home"></i>ADDRESS
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="expand" data-original-title="" title=""></a>

                        </div>
                    </div>

                    <div class="portlet-body"  style="display: none;">
                        <div class="row">
                            <div class="col-md-6">

                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        COUNTRY *
                                    </div>
                                    <div class="col-md-8 value">
                                        @Html.DropDownList("drpPrimaryCountry", (IEnumerable<SelectListItem>)ViewBag.Countries, "Select Country", new { @class = "form-control searchValue" ,@fieldName="priCountry" })
                                    </div>
                                </div>
                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        COMPANY
                                    </div>
                                    <div class="col-md-8 value">
                                        <input type="text" id="txtCompany" fieldName="priCompany" class="form-control searchValue" maxlength="100" />
                                    </div>
                                </div>
                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        ADDRESS 1
                                    </div>
                                    <div class="col-md-8 value">
                                        <input type="text" id="txtLine1" fieldName="priAddressLine1" class="form-control searchValue" maxlength="100" />
                                    </div>
                                </div>
                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        ADDRESS 2
                                    </div>
                                    <div class="col-md-8 value">
                                        <input type="text" id="txtLine2" fieldName="priAddressLine2" class="form-control searchValue" maxlength="100" />
                                    </div>
                                </div>
                                <div class="row static-info" id="divZipPrimary">
                                    <div class="col-md-4 name">
                                        STATE/ZIP
                                    </div>
                                    <div class="col-md-4 value">
                                        <select class="form-control searchValue" id="drpPrimaryStates" fieldName="priState">
                                            <option value>Please select</option>
                                        </select>
                                    </div>
                                    <div class="col-md-2 value">
                                        <input type="text" id="txtStateZip" class="form-control" maxlength="10" />
                                    </div>
                                    <div class="col-md-2 value">
                                        <input type="text" id="txtStateZip1" class="form-control" maxlength="10" />
                                    </div>
                                </div>
                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        CITY
                                    </div>
                                    <div class="col-md-8 value">
                                        <select class="form-control searchValue" id="drpPrimaryCites" fieldName="priCity">
                                            <option value>Please select</option>
                                        </select>
                                    </div>
                                </div>

                                
                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        CREDIT CARD
                                    </div>
                                    <div class="col-md-2 value">
                                        <input type="text" id="txtCreditCard1" class="form-control" maxlength="4" />
                                    </div>
                                    <div class="col-md-2 value">
                                        <input type="text" id="txtCreditCard2" class="form-control" maxlength="4" />
                                    </div>
                                    <div class="col-md-2 value">
                                        <input type="text" id="txtCreditCard3" class="form-control" maxlength="4" />
                                    </div>
                                    <div class="col-md-2 value">
                                        <input type="text" id="txtCreditCard4" class="form-control" maxlength="4" />
                                    </div>
                                </div>

                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        EXP
                                    </div>
                                    <div class="col-md-2 value">
                                        <input type="text" id="txtCardExpDay" class="form-control" maxlength="2" placeholder="Month" />
                                    </div>
                                    <div class="col-md-2 value">
                                        <input type="text" id="txtCardExpYear" class="form-control" maxlength="4" placeholder="Year" />
                                    </div>

                                </div>

                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        SECURITY
                                    </div>
                                    <div class="col-md-4 value">
                                        <input type="text" id="txtCardSecurity" class="form-control" maxlength="3" placeholder="CVV" fieldName="priCreditCardSecurity" />
                                    </div>
                                  
                                </div>




                            </div>
                            <div class="col-md-6">


                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        PRIMARY EMAIL
                                    </div>
                                    <div class="col-md-8 value">
                                        <input type="email" id="txtEmail" fieldName="priEMailAddress" class="form-control searchValue" maxlength="100" style="text-transform:none;" />
                                    </div>
                                </div>

                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        SECONDARY EMAIL
                                    </div>
                                    <div class="col-md-8 value">
                                        <input type="email" id="txtEmail2" fieldName="priEMailAddress2" class="form-control searchValue" maxlength="100" style="text-transform:none;" />
                                    </div>
                                </div>

                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        OFFICE PHONE
                                    </div>
                                    <div id="divUSPhonePrimary">
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtPhone1" class="form-control searchValue  clsPhoneTabToNext" fieldName="priTelephoneSegment1" maxlength="3"  />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtPhone2" class="form-control searchValue clsPhoneTabToNext" fieldName="priTelephoneSegment2" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtPhone3" class="form-control searchValue clsPhoneTabToNext" fieldName="priTelephoneSegment3" maxlength="4" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtPhone4" class="form-control searchValue clsPhoneTabToNext" fieldName="priTelephoneExtension" placeholder="EXT" maxlength="6" />
                                        </div>
                                    </div>

                                    <div id="divNonUSPhonePrimary" style="display: none;">
                                        <div class="col-md-8 value">
                                            <input type="text" id="txtPhone1NonUs" class="form-control txtPhone1 searchValue" fieldName="priTelephoneSegment1" maxlength="10" />
                                        </div>
                                    </div>
                                </div>

                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        HOME PHONE
                                    </div>
                                    <div id="divUSHOMEPhonePrimary">
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtHomePhone1" class="form-control txtHomePhone1 clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtHOMEPhone2" class="form-control clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtHomePhone3" class="form-control clsPhoneTabToNext" maxlength="4" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtHomePhone4" class="form-control clsPhoneTabToNext" placeholder="EXT" maxlength="6" />
                                        </div>
                                    </div>

                                    <div id="divNonUSHomePhonePrimary" style="display: none;">
                                        <div class="col-md-8 value">
                                            <input type="text" id="txtHomePhone1NonUs" class="form-control txtHomePhone1" maxlength="10" />
                                        </div>
                                    </div>
                                </div>



                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        ASSISTANT PHONE
                                    </div>
                                    <div id="divUSASSISTANTPhonePrimary">
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtASSISTANTPhone1" class="form-control txtAssistantPhone1 clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtASSISTANTPhone2" class="form-control clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtASSISTANTPhone3" class="form-control clsPhoneTabToNext" maxlength="4" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtASSISTANTPhone4" class="form-control clsPhoneTabToNext" placeholder="EXT" maxlength="6" />
                                        </div>
                                    </div>

                                    <div id="divNonUSASSISTANTPhonePrimary" style="display: none;">
                                        <div class="col-md-8 value">
                                            <input type="text" id="txtASSISTANTPhone1NonUs" class="form-control txtAssistantPhone1" maxlength="10" />
                                        </div>
                                    </div>
                                </div>


                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        CELL
                                    </div>
                                    <div id="divUSCellPrimary">
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtCall1" class="form-control txtCall1 clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtCall2" class="form-control clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtCall3" class="form-control clsPhoneTabToNext" maxlength="4" />
                                        </div>
                                    </div>

                                    <div id="divNonUSCellPrimary" style="display: none;">
                                        <div class="col-md-8 value">
                                            <input type="text" id="txtCall1NonUs" class="form-control txtCall1" maxlength="10" />
                                        </div>
                                    </div>
                                </div>

                                <div class="row static-info">
                                    <div class="col-md-4 name">
                                        FAX
                                    </div>

                                    <div id="divUSFaxPrimary">
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtFAX1" class="form-control txtFAX1 clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtFAX2" class="form-control clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                        <div class="col-md-2 value">
                                            <input type="text" id="txtFAX3" class="form-control clsPhoneTabToNext" maxlength="3" />
                                        </div>
                                    </div>

                                    <div id="divNonUSFaxPrimary" style="display: none;">
                                        <div class="col-md-8 value">
                                            <input type="text" id="txtFAX1NonUs" class="form-control txtFAX1" maxlength="10" />
                                        </div>
                                    </div>

                                </div>

                               


                                @* ashu *@


                                

                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
   
             <div class="text-right">
                        <button onclick="searchCustomer(1);" style="margin-bottom:10px;" class="btn green" type="button">Search</button>
                
        </div>
</div>
  <div class="row">
    <div class="col-md-12">
    <!-- BEGIN PAGE CONTENT-->


        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet box blue-hoki">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-grid"></i> Listing
                </div>
                <div class="tools">
                </div>
            </div>
            <div class="portlet-body">
                <input type="hidden" id="hdnPageNo" value="1" />
                <div class="table-scrollable">
                    <table class="table table-striped table-bordered table-hover" id="sample_1">
                        <thead>
                            <tr>
                                    <th>Full Name
                                    </th>
                                    <th>Primary Address
                                    </th>
                                    <th>City
                                    </th>
                                    <th>Primary Phone
                                    </th>

                                    @*<th>Action</th>*@
                                </tr>
                        </thead>
                        <tbody id="tbodyDetails">
                        </tbody>
                    </table>
                </div>
                <div class="row">
                    <div class="col-md-5 col-sm-12">
                        <div class="dataTables_info" id="sample_1_info" role="status" aria-live="polite"></div>
                    </div>
                    <div class="col-md-7 col-sm-12">
                        <div class="dataTables_paginate paging_simple_numbers" id="sample_1_paginate">
                            <ul class="pagination">
                                <li class="paginate_button previous" aria-controls="sample_1" tabindex="0" id="sample_1_previous"><a><i class="fa fa-angle-left"></i></a></li>
                                <li class="paginate_button next" aria-controls="sample_1" tabindex="0" id="sample_1_next"><a><i class="fa fa-angle-right"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <script id="tbodyTemplate" type="text/html">
                        <tr>
                            <td>
                                <a href="../Repair/NewRepair?id=${customerNumber}">
                                    ${FullName}
                                </a>
                            </td>
                            <td>${PrimaryAddress}
                            </td>
                            <td>${City}
                            </td>
                            <td>${PrimaryPhone}
                            </td>
                            @*<td>
                                <a  href="../Repair/NewRepair?id=${customerNumber}" class="btn default btn-xs blue-stripe">Choose </a>
                            </td>*@
                        </tr>
                </script>
            </div>
        </div>
        <!-- END EXAMPLE TABLE PORTLET-->

    </div>
</div>
<!-- END PAGE CONTENT-->
