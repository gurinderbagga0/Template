<div id="divRepairListByCustomerNumberPopUp" class="modal fade" tabindex="-1" data-width="90%" style="max-height:500px;">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Close</button>
        <h4 class="modal-title">Search Result</h4>
    </div>
    <div class="modal-body" >

         <div class="row">
    <div class="col-md-12">
    <!-- BEGIN PAGE CONTENT-->


        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet box blue-hoki">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-grid"></i> Listing
                </div>
                <div class="tools">
                </div>
            </div>
            <div class="portlet-body">
                <input type="hidden" id="hdnPageNoRepair" value="1" />
                <div class="table-scrollable">
                    <table class="table table-striped table-bordered table-hover" id="sample_1">
                        <thead>
                            <tr>
                                    <th>REPAIR NUMBER
                                    </th>
                                    <th>ENTRY DATE
                                    </th>
                                    <th>BRAND NAME
                                    </th>
                                    <th>SUPP. REP.#
                                    </th>
                                    <th>CURRENT STATUS
                                    </th>

                                    <th>Action</th>
                                </tr>
                        </thead>
                        <tbody id="tbodyDetailsRepair">
                        </tbody>
                    </table>
                </div>
                <div class="row">
                    <div class="col-md-5 col-sm-12">
                        <div class="dataTables_info" id="sample_1_infoRepair" role="status" aria-live="polite"></div>
                    </div>
                    <div class="col-md-7 col-sm-12">
                        <div class="dataTables_paginate paging_simple_numbers" id="sample_1_paginate">
                            <ul class="pagination">
                                <li class="paginate_button previous" aria-controls="sample_1" tabindex="0" id="sample_1_previousRepair"><a><i class="fa fa-angle-left"></i></a></li>
                                <li class="paginate_button next" aria-controls="sample_1" tabindex="0" id="sample_1_nextRepair"><a><i class="fa fa-angle-right"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <script id="tbodyTemplateRepair" type="text/html">
                        <tr>
                            <td>
                                <a  href="../Repair/NewRepair?id=${customerNumber}&Rid=${repairNumber}"> ${repairNumber} </a>
                               
                            </td>
                            <td>${EntryDate}
                            </td>
                            <td>${brandName}
                            </td>
                            <td>${supplierRepairNumber}
                            </td>
                             <td>${RepairStatus}
                            </td>
                            <td>
                                @*<a onclick="editExistingCustomer(${customerNumber});" class="btn default btn-xs blue-stripe">Choose </a>*@
                                <a  href="../Repair/NewRepair?id=${customerNumber}&Rid=${repairNumber}" class="btn default btn-xs blue-stripe">Choose </a>
                            </td>
                        </tr>
                </script>
            </div>
        </div>
        <!-- END EXAMPLE TABLE PORTLET-->

    </div>
</div>

         </div>
    <div class="modal-footer">
        <button type="button" data-dismiss="modal" class="btn btn-default">Close</button>
        @*<button type="button" class="btn blue" onclick="getListExistingCustomer(1);">Search</button>*@
        <input type="hidden" id="hdnid" value="0" />
